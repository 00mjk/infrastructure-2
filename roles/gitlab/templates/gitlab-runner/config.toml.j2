concurrent = 2
check_interval = 1
log_level = "info"
sentry_dsn = "{{ sentry_dsn_legacy }}"

[session_server]
  session_timeout = 1800

[[runners]]
  name = "BB-8"
  executor = "docker"
  url = "https://git.{{ domain }}"
  token = "{{ ci_runner_token }}"
  [runners.custom_build_dir]
  [runners.cache]
    [runners.cache.s3]
    [runners.cache.gcs]
  [runners.docker]
    image = "alpine:latest"
    tls_verify = false
    privileged = false
    disable_entrypoint_overwrite = false
    oom_kill_disable = false
    disable_cache = false
    volumes = ["/cache"]
    shm_size = 0
