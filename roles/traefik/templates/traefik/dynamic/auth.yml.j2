http:
  middlewares:
    auth:
      forwardAuth:
        address: 'http://authelia:9091/api/verify?rd=https://auth.{{ domain }}'
        trustForwardHeader: true
        authResponseHeaders:
          - Remote-User
          - Remote-Groups
          - Remote-Name
          - Remote-Email
    auth-basic:
      basicAuth:
        users:
          - "{{ vault.basicAuth.username }}:{{ vault.basicAuth.password }}"
